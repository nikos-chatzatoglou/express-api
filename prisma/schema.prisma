generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Article {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  createdAt DateTime @default(now())
  Author    User     @relation(fields: [authorId], references: [id])
  authorId  Int
}
model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  username String
  password String
  articles Article[]
}